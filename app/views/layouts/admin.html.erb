<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
<head>
  <title>Data Entry Administration</title>
  <meta http-equiv='content-type' content='text/html;charset=utf-8' />
  <%= stylesheet_link_tag "habitual", :media => "all" %>
  <%= stylesheet_link_tag "plain", :media => "all" %>
  <%= javascript_include_tag :defaults %>
  <% CalendarDateSelect.format=(:iso_date) %>
  <%= calendar_date_select_includes %>
  <%= javascript_include_tag 'tablesort_v2' %>

  <%= active_scaffold_includes %>
  <%= stylesheet_link_tag "as-overrides.css" %>
  <!--[if IE 6]>
    <%= stylesheet_link_tag "IE-6.0-iframe.css" %>
  <![endif]-->
</head>
<body id="admin">
  <div id="container" class="admin">
    <!-- <h1 id="header"><%= @page_title || "" %></h1> -->

    <!--
    <div id="sidebar" class="admin">
      <ul id="nav" class="admin">
        <li>
          <%= link_to "Data Entry", {:controller => :data, :action => 'index'}, :class => 'admin'%>
          <% if not current_page?(:action => 'index') %>
            <%#= link_to "Administer", {:controller => :admin, :action => 'index'}, :class => 'admin'%>
          <% end %>
        </li>
      </ul>
    </div>
    -->
    <div id="main" style="margin-right: 200px">
    <% if flash[:error] -%>
      <div id="error">
        <div id="errorExplanation"><%= flash[:error] %></div>
      </div>
    <% end -%>
    <% if flash[:notice] -%>
      <div id="notice"><%= flash[:notice] %></div>
    <% end -%>

      <%= yield :layout %>

      <% button_div do %>
        <% if current_page?(:action => 'export') || current_page?(:action => 'index') %>
          <%= button_elem(:go_back, "Back to Data Entry", "/data") %>
        <% else %>
          <%= button_elem(:go_back, "Go Back", "/admin") %>
        <% end %>
      <% end %>
    </div>
  </div>
</body>
</html>

